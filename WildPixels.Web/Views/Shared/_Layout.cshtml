@using WildPixels.Web.Components.Shared

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@((string)ViewBag.Title)</title>
    <link rel="icon" type="image/png" href="~/favicon.png">
    <base href="~/" />
    <link href="styles/reset.min.css" rel="stylesheet">
    <link href="styles/main.min.css" rel="stylesheet">
    <link href="EP_Quest.styles.css" rel="stylesheet" asp-append-version="true" />
    
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script src="lib/turn.min.js" type="text/javascript"></script>
    <script src="lib/zoom.min.js" type="text/javascript"></script>
    <script src="lib/scissor.min.js" type="text/javascript"></script>
    <script src="lib/hash.js" type="text/javascript"></script>
    <script src="scripts/ScrollContent.js" type="text/javascript"></script>
    <script src="scripts/Flipbook.js" type="text/javascript"></script>
    <script src="scripts/Cookies.js" type="text/javascript"></script>
</head>
<body>
    <main>
        <div class="particles">
            @for (var i = 0; i < 16; i++)
            {
                <span class="circle"></span>
            }
        </div>
        @RenderBody()
    </main>
    <script src="_framework/blazor.server.js"></script>
</body>
</html>